#include "../inc/p-lost.h"

void	drawBattleUnderlay(t_env *env, t_frame *frame)
{
	if (env, frame)
	{
		drawHorizontal(env, frame, frame->resX / 8, (frame->resX - frame->resX / 8), (frame->resX / 12));
		drawHorizontal(env, frame, frame->resX / 8, (frame->resX - frame->resX / 8), (frame->resX / 12) + (frame->resX / 6));
	}
}
